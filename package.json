{
  "name": "reachvet",
  "version": "0.5.14",
  "description": "Supply chain reachability analyzer - Check if vulnerable dependencies are actually used in your code",
  "main": "dist/index.js",
  "bin": {
    "reachvet": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taku-tez/ReachVet.git"
  },
  "keywords": [
    "security",
    "supply-chain",
    "reachability",
    "sbom",
    "vulnerability",
    "scanner",
    "cli",
    "sca"
  ],
  "author": "taku-tez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taku-tez/ReachVet/issues"
  },
  "homepage": "https://github.com/taku-tez/ReachVet#readme",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/picomatch": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.0.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^5.4.0",
    "chokidar": "^4.0.3",
    "commander": "^13.0.0",
    "glob": "^11.0.0",
    "p-limit": "^7.3.0",
    "picomatch": "^4.0.3",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module"
}
